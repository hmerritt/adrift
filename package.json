	"license": "UNLICENSED",
	"description": "Adrift | Template react app with batteries included",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsc && tsx bootstrap vite build",
		"dev": "yarn start",
		"lint": "tsc && eslint",
		"preview": "tsc && tsx bootstrap vite preview",
		"start": "tsx bootstrap vite",
		"test": "tsx bootstrap vitest run",
		"test:bench": "tsx bootstrap vitest bench",
		"test:coverage": "tsx bootstrap vitest run --coverage --silent",
		"test:e2e:setup": "yarn playwright install --with-deps",
		"test:e2e": "tsx bootstrap yarn playwright test",
		"test:watch": "tsx bootstrap vitest watch --ui --api 9527",
		"cosmos": "tsx bootstrap cosmos",
		"cosmos:export": "tsx bootstrap cosmos-export",
		"update": "yarn upgrade-interactive"
	},
