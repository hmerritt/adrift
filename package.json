		"workbox-strategies": "7.4.0"
	},
	"overrides": {
		"zod-validation-error": "4.0.2"
	},
	"browserslist": {
		"production": [
			">5%",
			"not dead"
		],
		"development": [
			">5%",
			"not dead"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hmerritt/adrift.git"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "bun@1.3.9"
}
