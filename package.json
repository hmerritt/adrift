{
	"name": "adrift",
	"version": "0.1.0",
	"license": "UNLICENSED",
	"description": "Adrift | Template react app with batteries included",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsc && node bootstrap.cjs vite build",
		"dev": "yarn start",
		"lint": "tsc",
		"preview": "tsc && node bootstrap.cjs vite preview",
		"start": "node bootstrap.cjs vite",
		"test": "node bootstrap.cjs vitest run",
		"test:bench": "node bootstrap.cjs vitest bench",
		"test:coverage": "node bootstrap.cjs vitest run --coverage",
		"test:e2e:setup": "yarn playwright install --with-deps",
		"test:e2e": "node bootstrap.cjs yarn playwright test",
		"test:watch": "node bootstrap.cjs vitest watch --ui",
		"update": "yarn upgrade-interactive"
	},
	"dependencies": {
