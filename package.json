{
	"name": "adrift",
	"version": "0.1.0",
	"license": "UNLICENSED",
	"description": "Adrift | Template react app with batteries included",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "bun run typecheck && bun run bootstrap vite build",
		"dev": "bun run bootstrap.ts vite",
		"lint": "bun run lint:fast && bun run lint:eslint && bun run typecheck",
		"lint:eslint": "eslint .",
		"lint:fast": "oxlint . --react-plugin --vitest-plugin --import-plugin",
		"lint:fix": "oxlint . --react-plugin --vitest-plugin --import-plugin --fix",
		"preview": "bun run typecheck && bun run bootstrap vite preview",
		"start": "bun run bootstrap vite",
		"test": "bun run bootstrap vitest run",
		"test:bench": "bun run bootstrap vitest bench",
		"test:coverage": "bun run bootstrap vitest run --coverage --silent",
		"test:e2e:setup": "bun playwright install --with-deps",
		"test:e2e": "bun playwright test",
		"test:watch": "bun run bootstrap vitest watch --ui --api 9527",
		"typecheck": "tsc --noEmit",
		"cosmos": "bun run bootstrap cosmos",
		"cosmos:export": "bun run bootstrap cosmos-export",
		"update": "bun update -i"
	},
	"dependencies": {
		"@dnd-kit/core": "6.3.1",
		"@dnd-kit/sortable": "10.0.0",
		"@dnd-kit/utilities": "3.2.2",
		"@stylexjs/stylex": "0.17.5",
		"@tanstack/pacer": "0.18.0",
		"@tanstack/react-router": "1.160.2",
		"@tanstack/react-store": "0.8.1",
		"@tanstack/router-plugin": "1.160.2",
		"@tanstack/store": "0.8.1",
		"animejs": "4.3.6",
		"dayjs": "1.11.19",
		"ky": "^1.14.3",
		"plausible-tracker": "0.3.9",
		"react": "19.2.4",
		"react-dom": "19.2.4"
	},
	"devDependencies": {
		"@eslint/compat": "2.0.2",
		"@babel/core": "7.29.0",
		"@babel/plugin-syntax-jsx": "7.28.6",
		"@babel/plugin-syntax-typescript": "7.28.6",
		"@nabla/vite-plugin-eslint": "2.0.6",
		"@playwright/test": "1.58.2",
		"@stylexjs/babel-plugin": "0.17.5",
		"@stylexjs/eslint-plugin": "0.17.5",
		"@tanstack/react-router-devtools": "1.160.2",
		"@testing-library/dom": "10.4.1",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.2",
		"@testing-library/react-hooks": "8.0.1",
		"@testing-library/user-event": "14.6.1",
		"@trivago/prettier-plugin-sort-imports": "6.0.2",
		"@types/bun": "^1.3.9",
		"@types/css": "0.0.38",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"@typescript-eslint/eslint-plugin": "8.56.0",
		"@typescript-eslint/parser": "8.56.0",
		"@vitejs/plugin-react": "5.1.4",
		"@vitest/coverage-v8": "4.0.18",
		"@vitest/ui": "4.0.18",
		"babel-plugin-react-compiler": "1.0.0",
		"cross-env": "10.1.0",
		"css": "3.0.0",
		"eslint": "10.0.0",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"eslint-plugin-react-hooks": "7.0.1",
		"globals": "17.3.0",
		"jsdom": "28.1.0",
		"oxlint": "1.48.0",
		"prettier": "3.8.1",
		"react-cosmos": "7.1.1",
		"react-cosmos-plugin-boolean-input": "7.1.1",
		"react-cosmos-plugin-open-fixture": "7.1.1",
		"react-cosmos-plugin-vite": "7.1.1",
		"react-scan": "0.5.0",
		"rollup-plugin-workbox": "8.1.3",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.56.0",
		"vite": "7.3.1",
		"vite-bundle-visualizer": "1.2.1",
		"vite-plugin-minify": "2.1.0",
		"vite-tsconfig-paths": "6.1.1",
		"vitest": "4.0.18",
		"workbox-build": "7.4.0",
